{
  "private": true,
  "scripts": {
    "prepare": "nitropack prepare && bun ./setup.js",
    "dev:linux": "concurrently \"redis-server\" \"bun sass\" \"nitropack dev\"",
    "dev:windows": "concurrently \"bun sass\" \"nitropack dev\"",
    "dev:bun": "bun ./run-dev.js",
    "dev:node": "node ./run-dev.js",
    "dev": "npm run dev:bun || npm run dev:node",
    "build": "nitropack build",
    "sass": "sass ./pages/styles/scss/main.scss ./pages/styles/style.css --watch",
    "start:bun": "bun .output/server/index.mjs",
    "start:node": "node .output/server/index.mjs",
    "start": "npm run start:bun || npm run start:node",
    "test:bun": "bun test --timeout 2000",
    "test:node": "node test --timeout 2000",
    "test": "npm run test:bun",
    "preview": "bun .output/server/index.mjs"
  },
  "dependencies": {
    "ioredis": "^5.3.2",
    "nitropack": "latest"
  },
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.10.4",
    "bun-types": "^1.0.15",
    "concurrently": "^8.2.2",
    "sass": "^1.69.5"
  }
}